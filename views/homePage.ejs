								<h3 class="box-title">Postman Code</h3>
							</div>
							<form class="form-horizontal form-group" action="/blogs" method="POST">
								<div class="box-body">
									<input class="form-control" name="code" type="text" placeholder="code" >
									<p></p>
									<div class="col-6-md">
										<button type="submit" class="btn btn-block btn-danger pull-right">Add</button>
									</div>
								</div>
							</form>
						</div>
					</div>
					<div class="col-md-6">
						<div class="box box-primary">
							<div class="box-header with-border">
								<h3 class="box-title">Upload Files</h3>
							</div>
							<div class="box-body">
								<form action="/file-upload" class="dropzone form-horizontal"  action="/blogs" method="POST" enctype="multipart/form-data">
									<div class="fallback">
										<input name='photos' type="file"/>
									</div> 
								</form>
							</div> 
						</div>  
					</div> 
					<div class="col-xs-12">
						<div class="box">
							<div class="box-header">
								<h3>
									Orders Status
								</h3>
							</div>
							<div class="box-body">
								<table id="example2" class="table table-bordered table-hover">
									<thead>
										<tr>
											<th>Customer Name</th>
											<th>Product Name</th>
											<th>Post Code</th>
											<th>Deliver Status</th>
											<th>Deliver Date<th>
										</tr>
									</thead>
									<tbody>
										<% orders.forEach(function(order){ %>
										<tr>
											<td><%= order.customerName %></td>
											<td><%= order.productName %></td>
											<td><%= order.barcode %></td>
											<td><%= order.status %></td>
											<td><%= order.deliverDate %></td>
										<tr>
										<% }) %>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</section>
